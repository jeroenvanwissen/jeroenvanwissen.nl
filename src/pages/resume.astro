              <span class="skillpill">Postman</span>
              <span class="skillpill">ChatGPT</span>
              <span class="skillpill">Claude</span>
              <span class="skillpill">OpenCode</span>
              <span class="skillpill">AI Assisted Development (AIAD)</span>
            </div>
          </div>
        </div>
      </aside>
    </div>
  </div>

  <style is:global>
    .page-wrapper-resume {
      display: grid;
      grid-template-columns: var(--content-width) var(--sidebar-width);
      gap: 4rem;
      max-width: 1440px;
      margin: 0 auto;
      padding: 2rem 2rem 4rem;
      justify-content: center;
    }

    .content-resume {
      grid-column: 1;
    }

    .sidebar-resume {
      grid-column: 2;
      position: relative;
    }

    .sidebar-inner {
      position: sticky;
      top: 7rem;
    }

    @media (max-width: 1060px) {
      .page-wrapper-resume {
        grid-template-columns: 1fr;
        max-width: var(--content-width);
        gap: 0;
      }
      .content-resume {
        grid-column: 1;
      }
      .sidebar-resume {
        display: none;
      }
    }

    /* Resume Sections in Sidebar */
    .resume-section {
      margin-top: 2rem;
      padding-top: 1.5rem;
      border-top: 1px solid var(--border-subtle);
    }

    .resume-section:first-child {
      margin-top: 0;
      padding-top: 0;
      border-top: none;
    }

    .resume-section-label {
      font-size: 0.7rem;
      font-weight: 600;
      text-transform: uppercase;
      letter-spacing: 0.1em;
      color: var(--text-muted);
      margin-bottom: 0.75rem;
    }
  </style>
</Layout>
